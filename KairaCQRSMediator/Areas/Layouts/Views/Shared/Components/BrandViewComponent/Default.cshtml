@using KairaCQRSMediator.Features.CQRS.Results.BrandResults
@model List<GetBrandQueryResult>
@*BrandViewComponent*@

<section class="logo-bar py-5 my-5">

            <div class="container">

            <div class="row">

                <div class="logo-content d-flex flex-wrap justify-content-between">
                @foreach (var item in Model)
                {
                    <img src="@item.Icon" alt="logo" class="logo-image img-fluid" style="width:200px;height:100px">
                }

                </div>

            </div>
        </div>


</section>
